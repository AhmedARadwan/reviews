{
  "root": true,
  "extends": [
    "next/core-web-vitals",
    "airbnb"
  ],
  "overrides": [{
    "files": ["**/*.ts", "**/*.tsx"],
    "extends": [
      "airbnb-typescript",
      "airbnb/hooks",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "react/jsx-props-no-spreading": "off"
    }
  }]
}
